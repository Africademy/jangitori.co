{
  "private": true,
  "scripts": {
    "dev": "next",
    "clean:dist": "rimraf .next ./tsconfig.tsbuildinfo",
    "build": "yarn format && yarn lint --fix && yarn clean:dist && yarn install && next build",
    "clean": "yarn clean:dist && rimraf yarn.lock node_modules && yarn cache clean",
    "refresh:all": "yarn clean && yarn install && yarn build",
    "start": "next start",
    "test:ts": "tsc",
    "lint": "eslint \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "analyze:unused": "next-unused",
    "analyze": "cross-env ANALYZE=true yarn run build",
    "test:mocha": "TS_NODE_PROJECT=\"tsconfig.test.json\" ./node_modules/mocha/bin/mocha -r ts-node/register ./src/**/*.spec.ts",
    "test:jest": "jest --clearCache && NODE_ENV=test jest",
    "test": "jest --clearCache && NODE_ENV=test jest",
    "test:utils": "jest --clearCache && NODE_ENV=test jest --group=utils --watchAll",
    "test:api": "jest --clearCache && NODE_ENV=test jest --group=api --watchAll",
    "deploy": "vercel --prod"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "yarn lint --max-warnings 0"
    ],
    "*.{ts,tsx,js,css,json,md,mdx}": [
      "yarn format"
    ]
  },
  "next-unused": {
    "alias": {},
    "debug": true,
    "include": [
      "src"
    ],
    "exclude": [
      ".test.",
      ".unit.",
      ".spec."
    ],
    "entrypoints": [
      "src/pages"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@react-hookz/web": "^12.0.4",
    "@sentry/nextjs": "^6.16.1",
    "@supabase/supabase-js": "^1.29.1",
    "@unly/simple-logger": "^1.0.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "downshift": "^6.1.7",
    "framer-motion": "^5.5.5",
    "lodash.times": "^4.3.2",
    "memoize-one": "^6.0.0",
    "mobx": "^6.3.10",
    "mobx-react-lite": "^3.2.2",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.1.1",
    "swr": "^1.1.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.7.0",
    "@emotion/babel-plugin": "^11.7.2",
    "@next/bundle-analyzer": "^12.0.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/chai": "^4.3.0",
    "@types/faker": "5.5.3",
    "@types/jest": "26.0.23",
    "@types/jest-expect-message": "1.0.3",
    "@types/lodash.times": "^4.3.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@zeit/next-source-maps": "^0.0.3",
    "autoprefixer": "^10.4.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "cypress": "^9.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-jest": "^25.3.3",
    "eslint-plugin-mobx": "^0.0.7",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "faker": "5.5.3",
    "husky": "^7.0.4",
    "jest": "26.6.3",
    "jest-expect-message": "1.0.2",
    "jest-extended": "^1.2.0",
    "jest-runner-groups": "^2.1.0",
    "jest-to-match-shape-of": "^1.3.1",
    "lint-staged": "^12.1.4",
    "mobx-logger": "^0.7.1",
    "mocha": "^9.1.3",
    "next-unused": "^0.0.6",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.8",
    "ts-jest": "26.5.6",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4"
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.1.1"
}
